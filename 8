local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("CometApply", "DarkTheme")
local Tab = Window:NewTab("Cool")
local Section = Tab:NewSection("Script things")

Section:NewButton("Print something.", "Print something.", function()
    print("lol")
end)
Section:NewSlider("Walkspeed", "SliderInfo", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
